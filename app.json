{
  "expo": {
    "name": "MusicPlay",
    "slug": "MusicPlay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MusioLogo.png",
    "scheme": "musicplay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MusicPlay"
    },
    "android": {
      "permissions": [
        "android.permission.READ_MEDIA_AUDIO",
        "READ_MEDIA_AUDIO",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MusioLogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.MusicPlay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-permissions",
        {
          "permissions": [
            "READ_MEDIA_AUDIO",
            "READ_EXTERNAL_STORAGE"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9e85fdfa-0d6a-48d8-bae6-c7eb19e1e345"
      }
    }
  }
}
